class Solution {
public:
    void reverseString(vector<char>& s) {
       int size = s.size();
       int index = size - 1;

        for (int i = 0 ; i < size ; i++) {
            if (i < index)
            {
            char temp = s[i];
            s[i] = s[index];
            s[index] = temp;
            index--;
            }
            else
            break;
        }
    }
};
